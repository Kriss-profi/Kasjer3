            <TextBox x:Name="SafeD_2000"      Grid.Column="2" Grid.Row="5"  AcceptsTab="False" Text="{Binding Path=DaySafe2000Quantity,   Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe2000Amount,   Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="21"/>
            <TextBox x:Name="SafeD_1000"      Grid.Column="2" Grid.Row="6"  AcceptsTab="False" Text="{Binding Path=DaySafe1000Quantity,   Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe1000Amount,   Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="22"/>
            <TextBox x:Name="SafeD_500"       Grid.Column="2" Grid.Row="7"  AcceptsTab="False" Text="{Binding Path=DaySafe500Quantity,    Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe500Amount,    Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="23"/>
            <TextBox x:Name="SafeD_200"       Grid.Column="2" Grid.Row="8"  AcceptsTab="False" Text="{Binding Path=DaySafe200Quantity,    Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe200Amount,    Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="24"/>
            <TextBox x:Name="SafeD_100"       Grid.Column="2" Grid.Row="9"  AcceptsTab="False" Text="{Binding Path=DaySafe100Quantity,    Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe100Amount,    Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="25"/>
            <TextBox x:Name="SafeD_50"        Grid.Column="2" Grid.Row="10" AcceptsTab="False" Text="{Binding Path=DaySafe50Quantity,     Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe50Amount,     Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="26"/>
            <TextBox x:Name="SafeD_20"        Grid.Column="2" Grid.Row="11" AcceptsTab="False" Text="{Binding Path=DaySafe20Quantity,     Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe20Amount,     Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="26"/>
            <TextBox x:Name="SafeD_10"        Grid.Column="2" Grid.Row="12" AcceptsTab="False" Text="{Binding Path=DaySafe10Quantity,     Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe10Amount,     Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="27"/>
            <TextBox x:Name="SafeD_5"         Grid.Column="2" Grid.Row="13" AcceptsTab="False" Text="{Binding Path=DaySafe5Quantity,      Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe5Amount,      Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="28"/>
            <TextBox x:Name="SafeD_2"         Grid.Column="2" Grid.Row="14" AcceptsTab="False" Text="{Binding Path=DaySafe2Quantity,      Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe2Amount,      Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="29"/>
            <TextBox x:Name="SafeD_1"         Grid.Column="2" Grid.Row="15" AcceptsTab="False" Text="{Binding Path=DaySafe1Quantity,      Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe1Amount,      Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="30"/>
            <TextBox x:Name="SafeM_50000"     Grid.Column="3" Grid.Row="1"  AcceptsTab="False" Text="{Binding Path=MainSafe50000Quantity, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=MainSafe50000Amount, Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="31"/>
            <TextBox x:Name="SafeM_20000"     Grid.Column="3" Grid.Row="2"  AcceptsTab="False" Text="{Binding Path=MainSafe20000Quantity, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=MainSafe20000Amount, Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="32"/>
            <TextBox x:Name="SafeM_10000"     Grid.Column="3" Grid.Row="3"  AcceptsTab="False" Text="{Binding Path=MainSafe10000Quantity, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=MainSafe10000Amount, Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="33"/>
            <TextBox x:Name="SafeM_5000"      Grid.Column="3" Grid.Row="4"  AcceptsTab="False" Text="{Binding Path=MainSafe5000Quantity,  Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=MainSafe5000Amount,  Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="34"/>
