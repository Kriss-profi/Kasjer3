            <TextBox x:Name="Casket_2"        Grid.Column="1" Grid.Row="14" AcceptsTab="False" Text="{Binding Path=Casket2Quantity,       Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=Casket2Amount,       Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="15"/>
            <TextBox x:Name="Casket_1"        Grid.Column="1" Grid.Row="15" AcceptsTab="False" Text="{Binding Path=Casket1Quantity,       Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=Casket1Amount,       Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="16"/>
            <TextBox x:Name="SafeD_50000"     Grid.Column="2" Grid.Row="1"  AcceptsTab="False" Text="{Binding Path=DaySafe50000Quantity,  Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe50000Amount,  Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="17"/>
            <TextBox x:Name="SafeD_20000"     Grid.Column="2" Grid.Row="2"  AcceptsTab="False" Text="{Binding Path=DaySafe20000Quantity,  Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=DaySafe20000Amount,  Mode=OneWay, StringFormat={}{0:N2} zł.}" TabIndex="18"/>
